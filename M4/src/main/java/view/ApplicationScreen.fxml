<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="applicationPage" alignment="center" prefHeight="432.0" prefWidth="421.0" spacing="40" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.ApplicationController">
    <padding><Insets bottom="10" left="25" right="25" top="25" /></padding>
    <children>
        <Label fx:id="applicationMessage" alignment="TOP_CENTER" prefHeight="23.0" prefWidth="221.0" text="Application">
            <font>
                <Font name="System Bold" size="18.0" />
            </font></Label>
        <Button fx:id="submitSourceReportButton" alignment="CENTER" onAction="#handleSubmitSourcePressed" text="Submit Source Report" textOverrun="CLIP">Submit Source Report</Button>
        <Button fx:id="viewSourceReportsButton" alignment="CENTER" onAction="#handleViewSourceReportsPressed">View Source Reports</Button>
        <Button fx:id="viewMapButton" mnemonicParsing="false" onAction="#handleViewMapPressed">View Map Report</Button>
        <Button fx:id="createButton" alignment="CENTER" onAction="#handleCreatePressed">Create Profile</Button>
        <Button fx:id="editButton" alignment="CENTER" onAction="#handleEditPressed">Edit Profile</Button>
        <Button fx:id="logoutButton" alignment="BOTTOM_CENTER" onAction="#handleLogoutPressed">Logout</Button>
        <Button fx:id = "submitReportButton" alignment = "BOTTOM_CENTER" onAction = "#handleSubmitReportPressed">Submit Water Report</Button>
        <Button fx:id = "viewPurityReportsButton" alignment = "BOTTOM_CENTER" onAction = "#handleViewPurityReportsPressed">View Purity Reports</Button>
    </children>
</VBox>
